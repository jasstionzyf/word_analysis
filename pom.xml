<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.baihe</groupId>
    <artifactId>baihe-analysis</artifactId>
    <packaging>war</packaging>
    <version>0.0.1-SNAPSHOT</version>
    <distributionManagement> 
        <snapshotRepository> 
            <id>nexusServer</id> 
            <name>nexus distribution snapshot repository</name> 
            <url>http://maven.baihe.com/nexus/content/repositories/snapshots/</url> 
            
             <snapshots>
            <updatePolicy>always</updatePolicy>
        </snapshots>
        
        
        </snapshotRepository> 
           <repository>
                    <id>nexusServer</id>
                    <url>http://maven.baihe.com/nexus/content/repositories/releases</url>
           </repository>
    </distributionManagement> 
    <repositories>
        <repository>
            <id>nexusServer</id>
            <name>nexus distribution snapshot repository</name> 
            <url>http://maven.baihe.com/nexus/content/repositories/snapshots/</url> 
           
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
            
        </repository>
         <repository>
            <id>central</id>
            <url>https://repo1.maven.org/maven2</url>
            <releases>
                <enabled>true</enabled>
            </releases>
        </repository>
        <repository>
            <id>cloudera</id>
            <url>https://repository.cloudera.com/artifactory/cloudera-repos/</url>
        </repository>
    </repositories>
     <pluginRepositories>
        <pluginRepository>
            <id>central</id>
            <url>https://repo1.maven.org/maven2</url>
            <releases>
                <enabled>true</enabled>
            </releases>
        </pluginRepository>
        
        <pluginRepository>
            <id>cloudera</id>
            <url>https://repository.cloudera.com/artifactory/cloudera-repos/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
        </pluginRepository>
    </pluginRepositories>
    <properties>
        <spring.version>4.1.6.RELEASE</spring.version>  
        <druid.version>1.0.12</druid.version>
        <servlet.version>2.5</servlet.version>
        <fastjson.version>1.1.43</fastjson.version>
        <slf4j.version>1.6.6</slf4j.version>
        <log4j.version>1.2.16</log4j.version>
        <xstream.version>1.2.2</xstream.version>
        <junit.version>4.10</junit.version>
        <maven-compiler-plugin>2.3.2</maven-compiler-plugin>
    </properties>

    <dependencies>
         <dependency>
            <groupId>com.baihe</groupId>
            <artifactId>utils</artifactId>
            <scope>compile</scope>

            <version>2.0</version>
        </dependency>
